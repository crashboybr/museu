<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
        <tr>
            <th style="width: 300px;">Título</th>
            <th>Subtítulo</th>
            <th>Veículo / Instituição</th>
            <th>Editoria / Programa</th>
            <th>Data</th>
            <th>Link</th>
        </tr>
    </thead>
    <tbody>
        {% for acervo in acervos %}
        <tr>
            <td>{{ acervo.title }}a</td>
            <td>{{ acervo.subtitle }}</td>
            <td>{{ acervo.vehicle }}</td>
            <td>{{ acervo.program }}</td>
            <td>{{ acervo.acervoDate|date('d/m/Y') }}</td>
            <td><a href="{{ acervo.url }}" target="_blank"><i class="fa fa-external-link"></i></a></td>
        </tr>
        {% endfor %}

    </tbody>    
</table>