<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
        <tr>
            <th style="width: 300px;">Título</th>
            <th style="width: 200px;">Diretor</th>
            <th style="width: 200px;">Produção</th>
            <th>Sinopse</th>
        </tr>
    </thead>
    <tbody>
        {% for acervo in acervos %}
        <tr>
            <td><a href="{{ path('museu_view_acervo', { 'id': acervo.id }) }}">{{ acervo.title }}</a></td>
            <td>{{ acervo.author }}</td>
            <td>{{ acervo.vehicle }}</td>
            <td>{{ acervo.sinopse }}</td>
        </tr>
        {% endfor %}

    </tbody>    
</table>