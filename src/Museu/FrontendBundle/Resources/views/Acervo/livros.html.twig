<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
        <tr>
            <th>Título</th>
            <th>Autor</th>
            <th>Editora</th>
            <th>Ano</th>
            <th>Gênero</th>
        </tr>
    </thead>
    <tbody>
        {% for acervo in acervos %}
        <tr>
            <td><a href="{{ path('museu_view_acervo', { 'id': acervo.id }) }}">{{ acervo.title }}</a></td>
            <td>{{ acervo.author }}</td>
            <td>{{ acervo.publisher }}</td>
            <td>{{ acervo.year }}</td>
            <td>{{ acervo.genre }}</td>
        </tr>
        {% endfor %}

    </tbody>    
</table>