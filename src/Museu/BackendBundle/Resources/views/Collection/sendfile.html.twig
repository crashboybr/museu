{% extends '::base.html.twig' %}

{% block body -%}
<div class="row-fluid">
	<div class="span12">
		<div class="portlet">
			<div class="portlet-body form">

				<h3 class="form-section">Enviar Imagens</h3>
				<span style="">
					{% for flashMessage in app.session.flashbag.get('success') %}
							{{ flashMessage }}
						{% endfor %}
				</span>
				<form method="POST" class="form-horizontal" enctype="multipart/form-data">
					<div class="control-group ">
						<label class="control-label" for="files">Arquivos</label>
						<div class="controls">
							
							<input type="file" multiple id="files" name="files[]" class="m-wrap large" value="1">
							
						</div>
					</div>
					<div class="control-group">
						<button type="submit">Enviar</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>

{% endblock %}
